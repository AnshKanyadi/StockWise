    <field id="category_selected"
          type="assocarray"
          alias="category_list.itemSelected"
        />
  </interface>
  <children>
 <Video
        id="musicvideos"
        width="1920"
        height="1080"
        translation="[0,0]"
        
/>
    <RadioButtonList
      id="category_list"
      translation="[580, 650]"
      itemSize="[550,200]"
      transparent="true"
      text-align="center"
      font-size="90"
      text-font="font:LargeBoldSystemFont"
      font-color="0xFFFFFFFF"
    >
      <ContentNode id="categories" role="content">
        <ContentNode title="Play"/>
        <ContentNode title="screen2"/>

      </ContentNode>
    </RadioButtonList>
  </children>
</component>
